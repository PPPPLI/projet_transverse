<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.petfinder.mapper.FeedbackMapper">

    <insert id="addFeedback" parameterType="Feedback">

        insert into feedback
            values (null,#{nomUtilisateur},#{mailUtilisateur},#{telUtilisateur},#{desciptionFeedback})

    </insert>



</mapper>